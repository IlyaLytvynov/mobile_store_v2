menuBg=yellow;

.navbar
	background menuBg
	.navbar_items
		display flex
		width 100%
		max-width 960px
		margin 0 auto
		.navbar_item > a
			display inline-block
			padding 15px 10px
			box-sizing border-box
			text-align center
			&:hover
				background rgba(0,0,0,.3)
				color #FFF
				transition all 0.3s

